<!-- gallery.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <title>მთავარი გალერეა</title>
</head>
<body>
    <h2>გალერეა</h2>
    <div class="gallery">
        <% images.forEach(image => { %>
            <img src="/uploads/<%= image %>" alt="Image">
        <% }); %>
    </div>
</body>
</html>

